msgctxt "b32d6755ca3246e7973fd093693df122"
msgid "༈  སྙིང་གཏམ་ སུམ་ཅུ་པ་ ཞེས་ བྱ་བ་ བཞུགས །"
msgstr ""

msgctxt "5efc42c2778d4aceb179f47f070a54b7"
msgid "ཆོས་དབྱིངས་ མཁའ་ཁྱབ་ ཡེ་ཤེས་ སྨོན་ལམ་ སྤྲིན་ཆེན་ ལ་␣ས །"
msgstr ""

msgctxt "570ebe68c83a4c23a1a743852ee1027a"
msgid "། ཐུགས་རྗེ་␣འི་ ཚ་ཟེར་ བདུད་རྩི་␣འི་ ཆུ་རྒྱུན་ ལེགས་ ཕབ་ ནས །"
msgstr ""

msgctxt "816a2c1672164309ab2b4aa53c35357d"
msgid "། གདུལ་བྱ་␣འི་ ཞིང་ དུ་ སྐུ་གསུམ་ མྱུ་གུ་ སྨིན་ མཛད་པ་␣འི །"
msgstr ""

msgctxt "d6da2278def34269a7ec2b4978a999aa"
msgid "། སྐྱབས་གསུམ་ མཆོག་གྱུར་ སྐྱོབ་ མཛད་ བླ་མ་␣འི་ ཞབས་ ལ་ འདུད །"
msgstr ""

msgctxt "4165ba77b1d84707815fafb87d528a3b"
msgid ""
"། སྨོན་ལམ་ སྟོབས་ ཀྱིས་ སྒྲུབ་བརྒྱུད་ དམ་པ་␣ས་ རྗེས་བཟུང་ ཡང༌ །། རང་ཉིད་ མི་"
" བརྩོན་ དོན་མེད་ དུས་ འདས་ ཚེ་ འདི་ གྲིབ་སོ་␣ར་ གཏུགས །"
msgstr ""

msgctxt "36c00465d41844c084fa12981e4319ab"
msgid "། དྲང་སྲོང་ སྤྱོད་ བསམ་ རང་ ལ་ ཡི་མུག་ དེ་ འདྲ་ གཞན་ ཡང་ མཐོང༌ །"
msgstr ""

msgctxt "fefdf957419540aa8fe33543e66aea8d"
msgid "། དེ་ཕྱིར་ རང་ བློ་ ངེས་འབྱུང་ བསྐུལ་ ཕྱིར་ སྙིང་གཏམ་ སུམ་ཅུ་ བརྗོད །།"
msgstr ""

msgctxt "aaf0a752d538436e88aa46d84f7bf96a"
msgid "ཀྱེ་ ཧོ་ སྣ་ཚོགས་ ཐབས་ ཀྱིས་ འཁོར་ མང་ བསྡུས་ ནས་ ནི །"
msgstr ""

msgctxt "eb8e91b4b35046a784482aab010c7981"
msgid "། མཐུན་ རྐྱེན་ འཛོམ་པ་␣འི་ དགོན་གཞི་␣ས་ འཛིན་པ་ ཡང༌ །"
msgstr ""

msgctxt "ef94fd09abfd481c8dafe552828ed36f"
msgid "། རྩོད་པ་␣འི་ གཞི་ དང་ བདག་ སེམས་ ཞེན་པ་␣འི་ རྒྱུ །"
msgstr ""

msgctxt "790a4aa14a264009889e231d31908f5f"
msgid "། གཅིག་པུ་␣ར་ སྡོད་པ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
msgstr ""

msgctxt "7195abf6d3f445f6a3a761df3823851c"
msgid "གྲོང་ཆོག་ སྲིའུ་ གསོ་ འདྲེ་འདུལ་ ལ་སོགས་ ཀྱི །"
msgstr ""

msgctxt "ebf9850e1f9e4d8797c387855f551ee4"
msgid "། ཡོན་ཏན་ སྐྱེ་བོ་␣འི་ དབུས་ སུ་ བྲིན་ གྱུར་ ཀྱང༌ །"
msgstr ""

msgctxt "528dea5690234f2e8345298b376cf78e"
msgid "། ཟས་ནོར་ སྲེད་པ་␣ས་ བདག་ བློ་ བདུད་ ཀྱིས་ ཁྱེར །"
msgstr ""

msgctxt "252e8410ccce48bd8fb1257785c21ad2"
msgid "། རང་སེམས་ འདུལ་བ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
msgstr ""

msgctxt "fc7845d0352c44fd8c701524008df37f"
msgid "དབུལ་ཕོངས་ རྣམས་ ལ་ཁྲལ་ ཆེན་ བསྡུས་ ནས་ ནི །"
msgstr ""

msgctxt "46f2f7b8f86140c6a2d4f4a660af0a78"
msgid "། རྟེན་ ཆེན་ བཞེངས་ དང་ ཡོ་ ལངས་ འགྱེད་ ལ་སོགས །"
msgstr ""

msgctxt "093ca6795b4e4a0eab75e3e090003b12"
msgid "། དགེ་ ལ་ བརྟེན་ ནས་ གཞན་ སྡིག་ གསོག་པ་␣འི་ རྒྱུ །"
msgstr ""

msgctxt "431fd81ffe034e2281ee2facc1ec00a4"
msgid "། རང་སེམས་ དགེ་བ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
msgstr ""

msgctxt "fa27958da6f44dd880e01e6a70fb37e3"
msgid "རང་ ཆེ་འདོད་ ཕྱིར་ གཞན་ ལ་ ཆོས་ བཤད་ དང༌ །"
msgstr ""

msgctxt "224715d57bd348488df5dbd224a2c562"
msgid "། ཐབས་མཁས་ ཆེན་པོ་␣ས་ མཆོག་ དམན་ འཁོར་ སྐྱོང་བ །"
msgstr ""

msgctxt "7dacd2bbca6b4743b6e615616375d6d4"
msgid "། དངོས་འཛིན་ སེམས་ ཀྱི་ ང་རྒྱལ་ སྐྱེ་བ་␣འི་ རྒྱུ །"
msgstr ""

msgctxt "0a31e6b28cba4cdb95dfe41b38280587"
msgid "། བློ་སྣ་ བསྟུང་བ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
msgstr ""

msgctxt "dab23119acf64239b3b738af09aebc8f"
msgid "ཚོང་ དང་ བུན་ སྐྱེད་ གཡོ་སྒྱུ་ ལ་སོགས་ ཀྱི །"
msgstr ""

msgctxt "039024840d65410694a36a3be4cc99f4"
msgid "། ལོག་འཚོ་␣ས་ བསགས་པ་␣འི་ ནོར་རྫས་ བརྒྱ་ འབུལ་ ཡང༌ །"
msgstr ""

msgctxt "618a246bb66e43c1acaa4e6f96286ddb"
msgid "། ལོ་ འདོད་ དགེ་བ་ ཆོས་ བརྒྱད་ དག་ གི་ རྒྱུ །"
msgstr ""

msgctxt "4509f7c5dae142dda873a2a476f01ea8"
msgid "། ཞེན་ ལོག་ སྒོམ་པ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
msgstr ""

msgctxt "e87610372b7845a3b2f8b64d19421827"
msgid "གཉའ་ དང་ དབང་པོ་ ཞལ་ལྕེ་ ལ་སོགས་ ཀྱིས །"
msgstr ""

msgctxt "3775ce9220aa450f9ad2f0f0ef8233da"
msgid "། གཞན་ རྩོད་ བསྡུམ་པ་ འགྲོ་དོན་ ཡིན་ སྙམ་ ཡང༌ །"
msgstr ""

msgctxt "1ae38c0fda4e49ea961690b75bad5fe0"
msgid "། དེ་ ལ་ བསྟེན་པ་␣འི་ ཞེ་འདོད་ འབྱུང་བ་␣འི་ རྒྱུ །"
msgstr ""

msgctxt "b066ee4e059a4f129531402e8cb7cf5c"
msgid "། རེ་དོགས་ མེད་པ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
msgstr ""

msgctxt "93c385892f8b49959f58d40dcf5dbf6e"
msgid "མངའ་རིས་ ལོངས་སྤྱོད་ འཁོར་ དང་ བསོད་ནམས་ དང༌ །"
msgstr ""

msgctxt "82979445c61444988fab32bc5d08e08d"
msgid "། སྙན་པ་␣འི་ གཏམ་ གྱིས་ འཛམ་གླིང་ གང་ གྱུར་ ཀྱང༌ །"
msgstr ""

msgctxt "131e915ad6854622aef195ec5a1b8278"
msgid "། རང་ཉིད་ འཆི་ ཚེ་ ཕན་པ་ གཅིག་ ཀྱང་ མེད །"
msgstr ""

msgctxt "d64a522ce48848bc9b1100392657d423"
msgid "། སྒྲུབ་ ལ་ བརྩོན་པ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
msgstr ""

msgctxt "e38cc8c1dac841f3bb6a89ab1307ee3c"
msgid "གཉེར་པ་ ཉེ་ གནས་ གཙོ་བོ་ ལ་སོགས་པ །"
msgstr ""

msgctxt "9ac4556166a848398f09fb336e5caa58"
msgid "། ཐབས་ ཁ་ གྲ་ ས་␣འི་ སྲོག་ཤིང་ ཡིན་མོད་ ཀྱང༌ །"
msgstr ""

msgctxt "26edb72659164f75a3b76a349fb0ada3"
msgid "། གཟུང་འཛིན་ བློ་ ཡི་ སེམས་ཁྲལ་ འབྱུང་བ་␣འི་ རྒྱུ །"
msgstr ""

msgctxt "1c3936641b404e3ca79ea2d6aa8ac81d"
msgid "། འདུ་འཛི་ བསྐྱུང་བ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
msgstr ""

msgctxt "b741895e243f401e86b6a3ebf20c827d"
msgid "རྟེན་ མཆོད་ དཔེ་ཆ་ ཐབས་ ཁ་ ལ་སོགས་པ །"
msgstr ""

msgctxt "2c3933e23460464aa0e60172ec5a96f2"
msgid "། ཅི་ དགོས་ ཁུར་ ནས་ རི་ཁྲོད་ འགྲིམས་པ་ ཡང༌ །"
msgstr ""

msgctxt "746ea6c6798545e9999c85dbe90b8244"
msgid "། འཕྲལ་ ལ་ འཛོམ་ སྟེ་ སྡུག་བསྔལ་ རྩོད་པ་␣འི་ རྒྱུ །"
msgstr ""

msgctxt "6907719710ed44a79631dea73d941de1"
msgid "། དགོས་ མེད་ བསྟེན་པ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
msgstr ""

msgctxt "3c017ac8d7ff44c79958d92e850813a6"
msgid "སྙིགས་མ་␣འི་ དུས་ འདི་␣ར་ དམུ་རྒོད་ འཁོར་ རྣམས་ ལ །"
msgstr ""

msgctxt "8b1073630bb24b758534a405d393c949"
msgid "། བཀའ་བཀྱོན་ ལ་སོགས་ ཕན་སེམས་ ཡིན་མོད་ ཀྱང༌ །"
msgstr ""

msgctxt "162ba105c33f49e6b1f47a5d8fa421f6"
msgid "། དེ་ ལ་ བརྟེན་ ནས་ ཉོན་མོངས་ སྐྱེ་བ་␣འི་ རྒྱུ །"
msgstr ""

msgctxt "65d29263257d4f1e8c9e2c8e0d589310"
msgid "། ཞི་བ་␣ར་ སྨྲ་བ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
msgstr ""

msgctxt "e46e9a11200f4e2a816dd16001c4fbe4"
msgid "ཕན་པ་␣འི་ སེམས་ ཀྱིས་ རང་འདོད་ མེད་པ་␣འི་ གཏམ །"
msgstr ""

msgctxt "8ce85f8614b641fd8063f4cab2400c7a"
msgid "། བརྩེ་བ་␣ས་ མི་ལ་ མཚང་ ནས་ བརྗོད་པ་ ཡང༌ །"
msgstr ""

msgctxt "2da2abc40ea14f61bf8c5e7b49af1dbd"
msgid "། དྲང་པོ་ ཡིན་ ཀྱང་ སྙིང་ སྐྲན་ འཇུག་པ་␣འི་ རྒྱུ །"
msgstr ""

msgctxt "1c529f8847e7463aba1052bea7526157"
msgid "། སྙན་པ་␣ར་ སྨྲ་བ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
msgstr ""

msgctxt "5d766cb905214c00adbd60016577c334"
msgid "རང་ཕྱོགས་ སྐྱོང་ ཞིང་ གཞན་ཕྱོགས་ དགག་པ་␣འི་ རྒྱུ །"
msgstr ""

msgctxt "840c166d110246509bd43b86c09f0211"
msgid "། བགྲོ་གླེང་ བསྟན་པ་␣འི་ གད་བདར་ ཡིན་ སྙམ་ ཡང༌ །"
msgstr ""

msgctxt "8d6656a028aa4b6e91002fe007d47f10"
msgid "། དེ་ ལ་ བརྟེན་ ནས་ ཉོན་མོངས་ སྐྱེ་བ་␣འི་ རྒྱུ །"
msgstr ""

msgctxt "f19f11a491e64155a70f581243bd2a81"
msgid "། སྨྲ་བཅད་ བྱེད་པ་ ཁོ་བོ་␣འི་ སྙིང་གཏམ་ ཡིན །།"
